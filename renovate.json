{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",  
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": ["java"],
      "matchUpdateTypes": ["major"],
      "enabled": false
    },
    {
      "matchManagers": ["gradle"],
      "matchPackagePatterns": ["com.google.guava:guava"],
      "enabled": false
    },
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    }
  ]
}
